{
  "name": "dynamic-tp-site",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm i",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:lint": "prettier --check --write docs/",
    "docs:clean-dev": "vuepress dev docs --no-cache",
    "packages:check-update": "pnpm dlx npm-check-updates -u --deep --timeout 600000",
    "packages:update": "pnpm dlx vp-update"
  },
  "dependencies": {
      "@mdit/plugin-container": "0.16.0",
      "@vuepress/bundler-vite": "2.0.0-rc.18",
      "@vuepress/plugin-docsearch": "2.0.0-rc.82",
      "chart.js": "^4.4.8",
      "sass": "^1.85.1",
      "vidstack": "^1.12.13",
      "vue": "^3.5.16",
      "vue3-carousel": "^0.14.0",
      "vuepress": "2.0.0-rc.18",
      "vuepress-theme-hope": "2.0.0-rc.70"
    },
   "devDependencies": {
      "markdownlint-cli2": "^0.17.2",
      "@vuepress/plugin-slimsearch": "2.0.0-rc.61",
      "prettier": "3.5.3"
    },
   "engines": {
      "node": ">=18.19.0"
    },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
