{
  "name": "dynamic-tp-site",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm i",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:lint": "prettier --check --write docs/",
    "docs:clean-dev": "vuepress dev docs --no-cache",
    "packages:check-update": "pnpm dlx npm-check-updates -u --deep --timeout 600000",
    "packages:update": "pnpm dlx vp-update"
  },
  "packageManager": "pnpm@8.15.1",
  "dependencies": {
    "@mdit/plugin-container": "0.8.0",
    "@vuepress/bundler-vite": "2.0.0-rc.6",
    "@vuepress/plugin-docsearch": "2.0.0-rc.11",
    "chart.js": "^4.4.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue3-carousel": "^0.3.1",
    "vuepress": "2.0.0-rc.6",
    "vuepress-theme-hope": "2.0.0-rc.24"
  },
  "devDependencies": {
    "prettier": "3.2.5"
  },
  "packageManager": "pnpm@8.15.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
